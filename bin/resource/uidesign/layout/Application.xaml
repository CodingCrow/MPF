            <!--ElementTree的项模版样式定义-->
            <DataTemplate x:Key="ElemItemTemplate" >
                <DockPanel>
                    <Rectangle Width="16" Height="16" VerticalAlignment="Center" Fill="{Binding Path=ElementIcon}" />
                    <TextBlock Name="UserIdGroup" Margin="2,0,8,0" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding Path=DisplayName }" />
                </DockPanel>
            </DataTemplate>

            <!--项目视图树的项模版样式定义-->
            <DataTemplate x:Key="RootItemTemplate" >
                <TextBlock Margin="2,0,8,0" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding Path=DisplayName }" />
            </DataTemplate>

            <!--公共树的项模版样式定义-->
            <DataTemplate x:Key="CommonItemTemplate" >
                <TextBlock Margin="2,0,8,0" HorizontalAlignment="Left" VerticalAlignment="Center" Text="{Binding Path=DisplayName }" />
            </DataTemplate>
            
            <Style TargetType="Button" x:Key="UnlockExpresionStyle">
                <Setter Property="Width" Value="11" />
                <Setter Property="Height" Value="17" />
                <Setter Property="VerticalAlignment" Value="Center" />
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Border Background="{TemplateBinding Background}">
                                <Rectangle Width="7" Height="7" VerticalAlignment="Center" HorizontalAlignment="Center"
                                           Stroke="#333333" Fill="#595959" StrokeThickness="1" />
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Style.Triggers>
                    <Trigger Property="IsPressed" Value="True">
                        <Setter Property="Background" Value="#AAAAAA" />
                    </Trigger>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Background" Value="#AAAAAA" />
                    </Trigger>
                </Style.Triggers>
            </Style>
            
            <!--带有图标的按钮风格-->
            <Style TargetType="RadioButton" x:Key="IconRadioButtonStyle">
                <Setter Property="Width" Value="32" />
                <Setter Property="Height" Value="32" />

                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="RadioButton">
                            <Border Background="{TemplateBinding Background}" >
                                <Rectangle Name="Icon" Fill="{TemplateBinding Tag}" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Fill" TargetName="Icon" Value="{TemplateBinding Tag1}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Background" Value="{content_bk}" />
                    </Trigger>
                    <Trigger Property="IsChecked" Value="True">
                        <Setter Property="Background" Value="{content_bk}" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <!--带有图标的按钮风格-->
            <Style TargetType="Button" x:Key="IconButtonStyle">
                <Setter Property="Width" Value="32" />
                <Setter Property="Height" Value="32" />
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Border Background="{TemplateBinding Background}" >
                                <Rectangle Name="Icon" Fill="{TemplateBinding Tag}" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Fill" TargetName="Icon" Value="{TemplateBinding Tag1}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Style.Triggers>
                    <Trigger Property="IsPressed" Value="True">
                        <Setter Property="Background" Value="{content_bk}" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style TargetType="ColorButton">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="ColorButton">
                            <Border Background="{TemplateBinding Color}" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            
            <Style x:Key="ColorSliderThumb" TargetType="Thumb">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Thumb">
                            <Path Fill="Black" Data="M0,0 L8,4 L0,8Z M20,0 L20,8 L12,4Z" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="ColorSlider">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="ColorSlider">
                            <Thumb Name="PART_Thumb" Cursor="Hand" Style="{ColorSliderThumb}" Width="20" Height="8" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="BrushElement" >
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="BrushElement">
                            <DockPanel Name="Bd" Orientation="Horizontal" >
                                <Grid Width="100" >
                                    <ContentPresenter Margin="0,0,5,0" HorizontalAlignment="Right" VerticalAlignment="Center" />
                                </Grid>
                                <Border BorderBrush="#353535" Background="{TemplateBinding Background}" BorderThickness="1" Margin="0,0,16,0" Height="18" VerticalAlignment="Center" />
                            </DockPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsChecked" Value="True">
                                    <Setter Property="Background" TargetName="Bd" Value="#BDBDBD" />
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" TargetName="Bd" Value="#666666" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="BrushSetterEditor">
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="BrushSetterEditor">
                            <BrushElement Name="PART_BrushElement" GroupName="BrushSetter_G" Content="{TemplateBinding Content}" Background="{TemplateBinding Background}" /> 
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
			
			<Style TargetType="ColorPickerBox">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="ColorPickerBox">
                            <Rectangle Fill="Green" /> 
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="ColorPicker">
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="ColorPicker">
                            <DockPanel >
								<DockPanel DockPanel.Dock="Bottom" Margin="0,0,0,2">
									<DockPanel Width="108" DockPanel.Dock="Right" >
										<ColorPickerBox Width="20" Height="20" Name="PART_PickColor" Cursor="Hand" >
										</ColorPickerBox>
										
										<DockPanel  Margin="8,0,0,0">
											<TextBlock Text="#" VerticalAlignment="Center" />
											<TextBox Name="PART_HexColor" Height="18" Margin="4,0,8,0" />
										</DockPanel>
									</DockPanel>
                                    <UniformGrid DockPanel.Dock="Bottom" Margin="0,0,0,2" Height="22" Columns="3" >
                                        <ColorButton Name="PART_Init" Margin="0,0,1,0" Color="{TemplateBinding InitColor}" />
                                        <ColorButton Name="PART_Last" Margin="0,0,1,0" Color="{TemplateBinding LastColor}" />
                                        <ColorButton Name="PART_Curr" Color="{TemplateBinding Color}" />
                                    </UniformGrid>
                                </DockPanel>
                                <StackPanel Margin="8,0,0,0" Width="80" DockPanel.Dock="Right">
                                    <DockPanel>
                                        <TextBlock Text="R" VerticalAlignment="Center" />
                                        <ColorChannel Name="PART_R" Height="18" Margin="4,0,8,0" />
                                    </DockPanel>
                                    <DockPanel Margin="0,4,0,4">
                                        <TextBlock Text="G" VerticalAlignment="Center" />
                                        <ColorChannel Name="PART_G" Height="18" Margin="4,0,8,0" />
                                    </DockPanel>
                                    <DockPanel>
                                        <TextBlock Text="B" VerticalAlignment="Center" />
                                        <ColorChannel Name="PART_B" Height="18" Margin="4,0,8,0" />
                                    </DockPanel>
                                    <DockPanel Margin="0,4,0,4">
                                        <TextBlock Text="A" VerticalAlignment="Center" />
                                        <ColorChannel Name="PART_A" Height="18" Margin="4,0,8,0" />
                                    </DockPanel>
                                </StackPanel>
                                     
								<DockPanel>
                                    <ColorSlider Name="PART_ColorSlider" DockPanel.Dock="Right" Width="20" />
                                    <ColorSelector Name="PART_Selector" Margin="0,0,1,0" />
                                </DockPanel>								
                            </DockPanel>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="RectEditor">
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="RectEditor">
                            <StackPanel ClipToBounds="True" >
                                <StackPanel Orientation="Horizontal" >
                                    <Rectangle Fill="{r_left}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Left"/>
                                    <IntegerControl Name="PART_Left" Width="35" Margin="2,0,8,0"/>
                                    <Rectangle Fill="{r_right}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Right"/>
                                    <IntegerControl Name="PART_Right" Width="35" Margin="2,0,8,0"/>
                                </StackPanel>
                                <StackPanel Margin="0,2,0,0" Orientation="Horizontal" >
                                    <Rectangle Fill="{r_up}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Top"/>
                                    <IntegerControl Name="PART_Top" Width="35" Margin="2,0,8,0"/>
                                    <Rectangle Fill="{r_down}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Bottom" />
                                    <IntegerControl Name="PART_Bottom" Width="35" Margin="2,0,8,0" />
                                </StackPanel>
                            </StackPanel>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="fRectEditor">
                <Setter Property="Template" >
                    <Setter.Value>
                        <ControlTemplate TargetType="fRectEditor">
                            <StackPanel ClipToBounds="True" >
                                <StackPanel Orientation="Horizontal" >
                                    <Rectangle Fill="{r_left}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Left"/>
                                    <FloatControl Name="PART_Left" Width="38" Margin="2,0,8,0"/>
                                    <Rectangle Fill="{r_right}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Right"/>
                                    <FloatControl Name="PART_Right" Width="38" Margin="2,0,8,0"/>
                                </StackPanel>
                                <StackPanel Margin="0,2,0,0" Orientation="Horizontal" >
                                    <Rectangle Fill="{r_up}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Top"/>
                                    <FloatControl Name="PART_Top" Width="38" Margin="2,0,8,0"/>
                                    <Rectangle Fill="{r_down}" Width="12" Height="12" VerticalAlignment="Center" ToolTip="Bottom" />
                                    <FloatControl Name="PART_Bottom" Width="38" Margin="2,0,8,0" />
                                </StackPanel>
                            </StackPanel>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

			<Style TargetType="ImageSourceEditor" >
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="ImageSourceEditor">
                            <Border Background="#333333" CornerRadius="5" >
                                <DockPanel>
                                    <Button Name="PART_EditSource" FontSize="9" Content="打开" Width="32" DockPanel.Dock="Right" />
