                                <Trigger Property="IsMouseOver" Value="True" >
                                    <Setter Property="Fill" TargetName="Icon" Value="{DynamicResource v_stretch_align_h}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Style.Triggers>
                    <Trigger Property="IsChecked" Value="True" >
                        <Setter Property="Background" Value="#FFBDBDBD" />
                    </Trigger>
                </Style.Triggers>
            </Style>

            <Style TargetType="AlignVertSetterEditor" >
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="AlignVertSetterEditor">
                            <StackPanel Orientation="Horizontal" Name="VerticalAlignment" Background="#333333" >
                                <RadioButton Name="Top" Margin="1" Style="{DynamicResource TopAlignStyle}" ToolTip="Top" GroupName="{TemplateBinding GroupName}" />
                                <RadioButton Name="Center" Margin="0,1,1,1" Style="{DynamicResource VCenterAlignStyle}" ToolTip="Center" GroupName="{TemplateBinding GroupName}" />
                                <RadioButton Name="Bottom" Margin="0,1,1,1" Style="{DynamicResource BottomAlignStyle}" ToolTip="Bottom" GroupName="{TemplateBinding GroupName}" />
                                <RadioButton Name="Stretch" Margin="0,1,1,1" Style="{DynamicResource VStretchAlignStyle}" ToolTip="Stretch" GroupName="{TemplateBinding GroupName}" IsChecked="True" />
                            </StackPanel>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="AlignHoriSetterEditor" >
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="AlignHoriSetterEditor">
                            <StackPanel Orientation="Horizontal" Name="VerticalAlignment" Background="#333333" >
                                <RadioButton Name="Left" Margin="1" Style="{DynamicResource LeftAlignStyle}" ToolTip="Left" GroupName="{TemplateBinding GroupName}" />
                                <RadioButton Name="Center" Margin="0,1,1,1" Style="{DynamicResource VCenterAlignStyle}" ToolTip="Center" GroupName="{TemplateBinding GroupName}" />
                                <RadioButton Name="Right" Margin="0,1,1,1" Style="{DynamicResource RightAlignStyle}" ToolTip="Right" GroupName="{TemplateBinding GroupName}" />
                                <RadioButton Name="Stretch" Margin="0,1,1,1" Style="{DynamicResource VStretchAlignStyle}" ToolTip="Stretch" GroupName="{TemplateBinding GroupName}" IsChecked="True" />
                            </StackPanel>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="ButtonSetterEditor" >
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="ButtonSetterEditor">
                            <Button Name="PART_Button" HorizontalAlignment="Right" Content="{TemplateBinding Content}" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
